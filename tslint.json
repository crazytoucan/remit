{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ],
  "rules": {
    "callable-types": false,
    "prefer-for-of": false,
    "member-access": true,
    "max-classes-per-file": false,
    "no-implicit-dependencies": [
      true,
      "dev"
    ],
    "unified-signatures": false
  }
}
